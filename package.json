{
  "name": "purescript-webpack-example",
  "version": "0.1.0",
  "private": true,
  "description": "Example using PureScript with webpack",
  "license": "MIT",
  "repository": "ethul/purescript-webpack-example",
  "author": {
    "name": "Eric Thul",
    "email": "thul.eric@gmail.com"
  },
  "files": [],
  "scripts": {
    "webpack": "DEBUG=purs-loader webpack --progress --bail",
    "webpack:watch": "webpack --progress --watch",
    "webpack:server": "webpack-dev-server --progress --inline --hot",
    "webpack:server:debug": "DEBUG=purs-loader webpack-dev-server --progress --inline --hot",
    "purs:compile": "purs compile '.psc-package/*/*/*/src/**/*.purs' 'src/**/*.purs'",
    "purs:bundle": "purs bundle output/**/*.js",
    "psc-package:update": "psc-package update",
    "start": "node bundle.js"
  },
  "dependencies": {
    "create-react-class": "^15.6.2",
    "psc-package": "^0.2.5",
    "purescript": "^0.11.0",
    "purescript-psa": "^0.5.0",
    "purs-loader": "^3.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.0.0"
  }
}
