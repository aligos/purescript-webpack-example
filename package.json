  "files": [],
  "scripts": {
    "purs:compile": "purs compile 'bower_components/purescript-*/src/**/*.purs' 'src/**/*.purs'",
    "webpack": "webpack --progress --bail",
    "webpack:dll": "webpack --verbose --colors --display-error-details --progress --config webpack-dll.config.js",
